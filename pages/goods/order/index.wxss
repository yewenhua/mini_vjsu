page{
    height:100%;
}
.page__bd{
    overflow: auto;
    height:100%;
}
.show{
    display: block!important;
}
.details{
    padding: 30rpx 0;
}
.details .btn{
    width: 100%;
    height: 88rpx;
    color: #fff;
    font-size: 32rpx;
    font-weight: 400;
    line-height: 88rpx;
    text-align: center;
    margin-bottom: 30rpx;
    background-image: linear-gradient(90deg, #7C92FF 0%, #22A3FE 100%);
    border-radius: 5px;
    border: none;
}
.sb-hover{
    background: #22A3FE;
}
.details .share{
    width: 600rpx;
    height: 108rpx;
    font-size: 32rpx;
    font-weight: 400;
    line-height: 108rpx;
    text-align: center;
    margin-bottom: 30rpx;
    border-radius: 200rpx;
    color: #333;
    background: #fff;
    border:2rpx solid #eee;
    background-image:none;
}
.details .share:after{
    border:none;
}
.sb-hover{
    background: #22A3FE;
}
.cells{
    background: none;
}
.cells .cell{
    display: flex;
    font-size: 28rpx;
    margin-bottom: 20rpx;
    line-height: 84rpx;
    border-radius: 0rpx;
    background-color: #fff;
}
.cells .cell .hd{
    padding: 0 30rpx;
}
.cells .cell .hd .weui-label {
    width: 75px;
}
.cells .cell .input{
    display:block;
    height:84rpx;
    text-overflow:clip;
    overflow:hidden;
    color: #333;
    white-space:nowrap;
    font-family:UICTFontTextStyleBody;
}
.cells .cell .bd{
    width: 100%;
}
.placeholder{
    color: #BBBBBB;
}
.share-confirm{
    position: absolute;
    left: 0;
    top: 0;
    width: 100%;
    height: 100%;
    display: none;
}
.share-confirm.show{
    display: block;
}
.share-confirm .mask{
    position: absolute;
    left: 0;
    top: 0;
    width: 100%;
    height: 100%;
    background: rgba(0,0,0,0.6);
    z-index:1;
}
.share-content{
    padding: 30rpx;
    width: 650rpx;
    height: 950rpx;
    border-radius: 20rpx;
    background: white;
    position: absolute;
    left: 50%;
    top: 50%;
    z-index: 99;
    transform: translate(-50%, -50%);
}
.share-content .title{
    font-size: 38rpx;
    font-weight: bold;
    color: #000;
    text-align: center;
    margin: 10rpx auto;
}
.share-content .desc{
    font-size: 24rpx;
    color: #777777;
    padding-left: 20rpx;
    padding-right: 20rpx;
    text-align: center;
}
.share-content .img{
    width: 100%;
    margin-top: 30rpx;
}
.share-content .img image{
    width: 100%;
}
.invite{
    font-size: 24rpx;
    color: #777777;
    padding: 30rpx;
    text-align: center;
}
.btnimg{
    width: 100%;
    display: flex;
    justify-content: center;
    align-items: center;
}
.btnimg image{
    width: 90%;
}
.close{
    position: absolute;
    top: 20rpx;
    right: 20rpx;
    width: 60rpx;
    height: 60rpx;
}
.close image{
    width: 100%;
}
.shareclick{
    width: 90%;
    background: none;
    height: 90rpx;
    background-image: linear-gradient(90deg, #7C92FF 0%, #22A3FE 100%);
    border-radius: 5px;
    color: white;
    font-size: 32rpx;
    margin-bottom: 50rpx;
}
.jd{
    margin-top: 30rpx;
    width: 100%;
    display: flex;
    justify-content: center;
    align-items: center;
}
.jd image{
    width: 40%;
}
.picker{
  color: #333;
}

.dxb{
  padding-left: 30rpx;
  position: relative;
  width: 100%;
  box-sizing: border-box;
}
.dxb:before{
  content: '';
  position: absolute;
  right: 40rpx;
  top: 50%;
  width: 20rpx;
  height: 20rpx;
  border-top: 2rpx solid #ddd;
  border-right: 2rpx solid #ddd;
  transform: rotate(45deg) translateY(-50%);
}


.citypickers{
  position: fixed;
  height: 100%;
  width: 100%;
  min-height: 100%;
  background-color: red;
}

.citypicker{
  height:100%;
  width:100%;
  position:absolute;
  left:0;
  top:0;
  z-index:9999; 
}
.citybody {
  position: fixed;
  bottom: 0px;
  width: 100%; 
  height: 560rpx; 
  z-index:99999; 
  background: white; 
}

.cityheader {
  position: absolute;
  top:0px;
  width: 100%;
  z-index: 4;
  background:white;
  border-bottom: 2rpx #f8f8f8 solid;
}
.mask{
   background:rgba(0,0,0,0.6);
   height:100%;
   width:100%;
}
.city-cancel {
  float: left;
  margin: 20rpx;
  color: #818181;
}

.city-true {
  float: right;
  margin: 20rpx;
  color: #2FB42E
}

.section .picker {
  background-color: #fff;
  border-bottom: 1px #d9d9d9 solid;
  border-top: 1px #d9d9d9 solid;
  padding: 20rpx;
}

.relative{
  position: relative;
}
.getcode{
  position: absolute;
  right: 0rpx;
  top: 50%;
  transform: translateY(-50%);
  height: 84rpx;
  line-height: 84rpx;
  font-size: 28rpx;
  color: #666;
  background: white;
  z-index: 9999;
  padding-left: 30rpx;
  padding-right: 30rpx;
}
.mutil{
    font-size: 26rpx;
    line-height: 32rpx;
}
.diy.hd{
    display: flex;
    align-items: center;
}
.protocal{
    margin-top: 16rpx;
}
.p-text{
    height: 50rpx;
    line-height: 50rpx;
}
.drawer_box { 
    width: 100%; 
    height: 100%;
    overflow: hidden; 
    position: fixed; 
    top: 0; 
    left: 0; 
    z-index: 1001; 
    margin-left: 0rpx;
    margin-top: 0rpx;
    border-radius: 0px; 
    background:white;
} 
.close{
    position: absolute;
    right: 30rpx;
    top:12rpx;
    width: 60rpx;
    z-index: 999;
}
.result{
    height: calc( 100% - 60rpx);
    margin-top: 80rpx;
    padding-bottom: 40rpx;
    box-sizing: border-box;
    overflow: auto;
}
.pro-text{
  color: #22A3FE;
}
.pro-title{
  position: absolute;
  top: 20rpx;
  left: 0;
  width: 100%;
  display: flex;
  justify-content: center;
  align-items: center;
  z-index: 9;
}
.result .title{
  font-weight: bold;
  font-size: 30rpx;
  padding-left:30rpx;
  padding-right:30rpx;
  margin-top: 12rpx;
}
.result .content{
  font-size: 26rpx;
  padding-left:30rpx;
  padding-right:30rpx;
}
.result .sub-content{
  font-size: 26rpx;
  text-indent: 30rpx;
  padding-left:30rpx;
  padding-right:30rpx;
}